{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "jest": true,
    "node": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended", "plugin:prettier/recommended"],
  "rules": {
    "no-console": "warn",
    "no-unused-vars": "error",
    "react/no-unescaped-entities": "off",
    "react/react-in-jsx-scope": "off"
  },
}
